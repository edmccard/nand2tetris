/**
 * 16-way Or: 
 * out = (in[0] or in[1] or ... or in[15])
 */

CHIP Or16Way {
    IN in[16];
    OUT out;

    PARTS:
    Or(a=a0, b=a1, out=out);
    Or8Way(in=in[0..7], out=a0);
    Or8Way(in=in[8..15], out=a1);
}